	Doi = {10.1016/j.cell.2012.05.044},
	Journal = {Cell},
	Journal-Full = {Cell},
	Mesh = {Bacterial Proteins; Cell Cycle; Computer Simulation; DNA-Binding Proteins; Models, Biological; Molecular Sequence Annotation; Mycoplasma genitalium; Phenotype},
	Month = {Jul},
