	Pst = {ppublish},
	Title = {A kinetic study of the lactic acid fermentation. Batch process at controlled pH. Reprinted from Journal of Biochemical and Microbiological Technology Engineering Vol. I, No. 4. Pages 393-412 (1959)},
	Volume = {67},
	Year = {2000}}

